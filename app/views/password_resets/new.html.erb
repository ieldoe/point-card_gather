<%= form_with url: password_resets_path, local: true do |f| %>
        <div class="form-group">
          <%= f.label :email, User.human_attribute_name(:email) %><br />
          <%= f.email_field :email, class: 'form-control' %>
        </div>
        <%= f.submit "リセットメール送信",class: 'btn btn-primary' %>
      <% end %>
